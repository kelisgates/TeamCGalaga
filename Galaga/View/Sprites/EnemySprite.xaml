<local:BaseSprite
    x:Class="Galaga.View.Sprites.EnemySprite"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Galaga.View.Sprites"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="55"
    d:DesignWidth="60">

    <Canvas>
        <Ellipse Height="40" Width="60" Canvas.Top="10" Stroke="White" Fill="AliceBlue"/>
        <Ellipse Height="10" Width="30" Canvas.Top="45" Canvas.Left="15" Stroke="Yellow" Fill="YellowGreen"/>
        <Rectangle Height="10" Width="60" Canvas.Top="40" Stroke="GhostWhite" Fill="CadetBlue"/>
        <Line X1="15" Y1="0" X2="25" Y2="20" StrokeThickness="2" Stroke="Yellow"/>
        <Line X1="45" Y1="0" X2="35" Y2="20" StrokeThickness="2" Stroke="Yellow"/>

    </Canvas>
</local:BaseSprite>
