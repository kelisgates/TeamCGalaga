<local:BaseSprite 
    x:Class="Galaga.View.Sprites.EnemyL2"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Galaga.View.Sprites"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="55"
    d:DesignWidth="60"
    Height="55"
    Width="60">

    <Canvas>
        
        <Ellipse Height="40" Width="60" Canvas.Top="10" Stroke="White" Fill="AliceBlue"/>
        <Ellipse Height="10" Width="30" Canvas.Top="45" Canvas.Left="15" Stroke="Violet" Fill="BlueViolet"/>
        <Rectangle Height="10" Width="60" Canvas.Top="40" Stroke="GhostWhite" Fill="OrangeRed"/>
        <Line X1="15" Y1="0" X2="25" Y2="20" StrokeThickness="2" Stroke="Violet"/>
        <Line X1="45" Y1="0" X2="35" Y2="20" StrokeThickness="2" Stroke="Violet"/>
        <Ellipse Height="20" Width="10" Canvas.Top="20" Canvas.Left="5" Fill="Black" Stroke="white"/>
        <Ellipse Height="20" Width="10"  Canvas.Top="20" Canvas.Left="25" Fill="Black" Stroke="white"/>
        <Ellipse Height="20" Width="10"  Canvas.Top="20" Canvas.Left="45" Fill="Black" Stroke="white"/>
    </Canvas>
</local:BaseSprite>
